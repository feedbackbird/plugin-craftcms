{
    "name": "feedbackbird/widget-craftcms",
    "description": "Display the FeedbackBird widget in your CraftCMS website and admin. Collect valuable feedback effortlessly and enhance user engagement. Streamline feedback management with ease.",
    "type": "craft-plugin",
    "license": "mit",
    "support": {
        "email": "support@veronalabs.com",
        "issues": "https://github.com/feedbackbird/widget-craftcms/issues?state=open",
        "source": "https://github.com/feedbackbird/widget-craftcms",
        "docs": "https://github.com/feedbackbird/widget-craftcms",
        "rss": "https://github.com/feedbackbird/widget-craftcms/releases.atom"
    },
    "require": {
        "php": ">=8.0.2",
        "craftcms/cms": "^4.4.0"
    },
    "require-dev": {
        "craftcms/ecs": "dev-main",
        "craftcms/phpstan": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "FeedbackBird\\Widget\\": "src/"
        }
    },
    "extra": {
        "handle": "feedbackbird",
        "name": "FeedbackBird",
        "developer": "VeronaLabs",
        "documentationUrl": "https://github.com/feedbackbird/widget-craftcms",
        "class": "FeedbackBird\\Widget\\FeedbackBird"
    },
    "scripts": {
        "check-cs": "ecs check --ansi",
        "fix-cs": "ecs check --ansi --fix",
        "phpstan": "phpstan --memory-limit=1G"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}

